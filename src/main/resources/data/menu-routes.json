[
  {
    "path": "/example",
    "component": "Layout",
    "redirect": "/example/list",
    "name": "Example",
    "meta": {
      "title": "example",
      "icon": "el-icon-s-help",
      "roles": [
        "ADMIN"
      ]
    },
    "children": [
      {
        "path": "create",
        "component": "example/create",
        "name": "CreateArticle",
        "meta": {
          "title": "createArticle",
          "icon": "edit",
          "roles": [
            "ADMIN"
          ]
        }
      },
      {
        "path": "edit/:id(\\d+)",
        "component": "example/edit",
        "name": "EditArticle",
        "meta": {
          "title": "editArticle",
          "noCache": true,
          "activeMenu": "/example/list",
          "roles": [
            "ADMIN"
          ]
        },
        "hidden": true
      },
      {
        "path": "list",
        "component": "example/list",
        "name": "ArticleList",
        "meta": {
          "title": "articleList",
          "icon": "list",
          "roles": [
            "ADMIN"
          ]
        }
      }
    ]
  }
]